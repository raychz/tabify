{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/.well-known/apple-developer-merchantid-domain-association.txt",
        "destination": "/assets/.well-known/apple-developer-merchantid-domain-association.txt"
      },
      {
        "source": "/privacy",
        "destination": "/assets/privacy/privacy.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}